<UserControl x:Class="_02350Project.View.EdgeUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Line Stroke="Black" StrokeThickness="1" StrokeDashArray="5,2" X1="{Binding EndA.North.X}" Y1="{Binding EndA.North.Y}" X2="{Binding EndB.North.X}" Y2="{Binding EndB.North.Y}"/> 
</UserControl>
